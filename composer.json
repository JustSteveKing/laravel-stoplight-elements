{
    "name": "juststeveking/laravel-stoplight-elements",
    "description": "A simple API documentation package for Laravel using OpenAPI and Stoplight Elements",
    "keywords": [
        "laravel",
        "api",
        "documentation",
        "stoplight",
        "stoplight elements",
        "openapi"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "role": "developer",
            "name": "Steve McDougall",
            "email": "juststevemcd@gmail.com",
            "homepage": "https://www.juststeveking.uk/"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "JustSteveKing\\Laravel\\LaravelStoplight\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JustSteveKing\\Laravel\\LaravelStoplight\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2|^8.3",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9|^10.0",
        "phpunit/phpunit": "^10|^11.5.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "JustSteveKing\\Laravel\\LaravelStoplight\\StoplightServiceProvider"
            ]
        }
    }
}
